# Nome da aplicação (só informativo)
VITE_APP_NAME="11 PromptLab"

# OpenAI
# Base URL padrão (não altere a menos que saiba o que está fazendo)
VITE_OPENAI_BASE_URL="https://api.openai.com"

# Modelo padrão (pode ser sobrescrito na UI)
VITE_MODEL_DEFAULT="gpt-4o-mini"

# Preferências
VITE_LOCALE="pt-BR"
VITE_DEFAULT_RETRIES="10"

# Batch (defaults que podem ser sobrescritos na UI)
VITE_BATCH_DEFAULT_CONCURRENCY="2"
VITE_BATCH_DEFAULT_OVERWRITE="false"
VITE_BATCH_DEFAULT_FAIL_ON_ERROR="false"

# ⚠️ NÃO COMITE API KEYS.
# Para desenvolvimento local, use .env.local e defina:
# VITE_OPENAI_API_KEY="coloque_sua_chave_aqui"
